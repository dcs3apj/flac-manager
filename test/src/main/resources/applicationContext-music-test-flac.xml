<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd"
  default-autowire="byName">

	<bean id="testFlacProvider" class="uk.co.unclealex.music.test.TestFlacProvider"/>
	<bean id="flacTrackDao" class="uk.co.unclealex.music.test.TestFlacTrackDao"/>
	<bean id="flacAlbumDao" class="uk.co.unclealex.music.test.TestFlacAlbumDao"/>
	<bean id="flacArtistDao" class="uk.co.unclealex.music.test.TestFlacArtistDao"/>
	<bean id="slimServerConfig" class="uk.co.unclealex.music.test.TestSlimServerConfig">
		<property name="definiteArticles">
			<list>
				<value>The</value>
				<value>La</value>
			</list>
		</property>
	</bean>

	<alias name="testFlacProvider" alias="externalCoverArtService"/>
		
	<bean id="slimServerInformationDao"
		class="uk.co.unclealex.music.test.TestSlimServerInformationDao"/>
</beans>

